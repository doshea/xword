.row.row-topper.row-bookend
  %h5
    Search Results
.row#search-results
  .large-12.columns
    - if @crosswords.any?
      = render partial: 'pages/partials/crossword_search'
      %hr
    - if @users.any?
      = render partial: 'pages/partials/user_search'
      %hr
    - if @words.any?
      = render partial: 'pages/partials/word_search'
      %hr
.row.row-stopper.row-bookend